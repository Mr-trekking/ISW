FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U   (0 0 0)
);

regions
(
    boxToCell
    {
        box (0 0 0) (150.0 5.0 0.5);
        fieldValues
        (
            volVectorFieldValue U (1 0 0)
        );
    }
);


